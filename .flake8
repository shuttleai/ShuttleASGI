[flake8]
ignore = E203, E266, W503, E704, E701, F824
max-line-length = 88
max-complexity = 18
per-file-ignores =
    shuttleasgi/__init__.py:F401
    shuttleasgi/client/__init__.py:F401
    shuttleasgi/server/__init__.py:F401
    shuttleasgi/url.py:E501
    tests/*:E501
    itests/*:E501
    itests/conftest.py:F401,F403

exclude =
    env
    .env
    venv
    venv311
    build
    .eggs
